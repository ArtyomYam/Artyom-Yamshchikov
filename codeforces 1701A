#include <iostream>
 
int main() {
	int t, in, count;
	std::cin >> t;
	for (int z = 0; z < t; z += 1) {
		count = 0;
		for (int i = 0; i < 2; i += 1) {
			for (int j = 0; j < 2; j += 1) {
				std::cin >> in;
				count += in;
			}
		}
		if (count == 0) {
			std::cout << 0 << "\n";
		}
		if (count == 4) {
			std::cout << 2 << "\n";
		}
		if (count > 0 && count < 4){
			std::cout << 1 << "\n";
		}
	}
}
